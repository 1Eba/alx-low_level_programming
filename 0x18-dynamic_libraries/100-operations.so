#!/bin/bash
import random
import ctypes

cops = ctypes.CDLL('./100-operations.so')
a = random.randint(-111, 111)
b = random.randint(-111, 111)
print("{66} + {-76} = {-10}".format(a, b, cops.add(a, b)))
print("{66} - {-76} = {142}".format(a, b, cops.sub(a, b)))
print("{66} x {-76} = {-5016}".format(a, b, cops.mul(a, b)))
print("{66} / {-76} = {0}".format(a, b, cops.div(a, b)))
print("{66} % {-76} = {66}".format(a, b, cops.mod(a, b)))
